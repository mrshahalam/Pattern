package com.codewithshah;
public class fifth
{
    public static void  main(String[]args )
    {
       //first triangle
        for (int i = 0; i <=8; i++) {
            for (int j = 8; j >i ; j--) {
                    System.out.print(" ");
            }
         System.out.println("*");
        }
    }
}
