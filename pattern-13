package test;

import java.util.*;

public class next {
	
	 public static void main(String[] args) 
	 {
		 Scanner sc = new Scanner(System.in);
		 //
		 for(int i=1;i<=7;i++)
		 {
			 
			for(int j=1;j<=i;j++)
			{
				System.out.print(j);
				
			}
      
      // for second half
			for (int j = i-1; j >= 1; j--)
            {
                System.out.print(j);
            }
			
			
			 System.out.println();
			 
			 
		 }
	 }
}


