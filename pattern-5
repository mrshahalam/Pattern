package test;

import java.util.*;

public class next {
	
	public static void main(String[] args)
	{
		int rows=6;
		for(int i=1;i<=rows;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print(" *");
			}
			System.out.println();
		}
	
	
		for(int l=5;l>=1;l--)
		{
			for(int m=1;m<=l;m++)
			{
				System.out.print(" *");
			}
		 System.out.println();	
		}
	}
}


